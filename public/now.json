{
    "version": 2,
    "alias": "digital-greenhouse",
    "name": "greenhouse-app",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "build": {
        "env": {
            "SERVER_ENDPOINT": "https://greenhouse-server23823.herokuapp.com/"
        }
    }
  }